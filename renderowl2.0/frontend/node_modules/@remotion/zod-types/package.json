{
	"repository": {
		"url": "https://github.com/remotion-dev/remotion/tree/main/packages/zod-types"
	},
	"name": "@remotion/zod-types",
	"version": "4.0.429",
	"description": "Zod types for Remotion",
	"main": "dist/cjs/index.js",
	"types": "dist/cjs/index.d.ts",
	"module": "dist/esm/index.mjs",
	"sideEffects": false,
	"scripts": {
		"formatting": "oxfmt src --check",
		"lint": "eslint src",
		"make": "tsgo -d && bun --env-file=../.env.bundle bundle.ts"
	},
	"author": "Jonny Burger",
	"contributors": [],
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/remotion-dev/remotion/issues"
	},
	"dependencies": {
		"remotion": "4.0.429"
	},
	"peerDependencies": {
		"zod": "4.3.6"
	},
	"devDependencies": {
		"zod": "4.3.6",
		"@remotion/eslint-config-internal": "4.0.429",
		"eslint": "9.19.0",
		"@typescript/native-preview": "7.0.0-dev.20260217.1"
	},
	"keywords": [
		"zod",
		"color",
		"remotion"
	],
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": "./dist/cjs/index.d.ts",
			"module": "./dist/esm/index.mjs",
			"import": "./dist/esm/index.mjs",
			"require": "./dist/cjs/index.js"
		}
	},
	"homepage": "https://www.remotion.dev/docs/zod-types"
}
